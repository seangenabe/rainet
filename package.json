{
  "name": "rainet",
  "version": "1.0.0",
  "description": "A JavaScript implementation of 5pb.'s game RaiNet Access Battlers.",
  "private": "true",
  "main": "lib",
  "scripts": {
    "test": "node --harmony_rest_parameters node_modules/mocha/bin/_mocha --compilers js:babel-core/register src/test",
    "compile": "babel --out-dir lib src",
    "watch": "npm run compile -- --watch",
    "jsdoc": "jsdoc -c .jsdoc.json --readme readme.md --destination docs src",
    "jsdoc:watch": "chokidar --command \"npm run jsdoc\" src"
  },
  "author": "Sean Genabe <seangenabe@outlook.com>",
  "license": "MIT",
  "engines": {
    "node": ">=5",
    "npm": ">=2"
  },
  "dependencies": {
    "babel-runtime": "^5.8.25",
    "immutable": "^3.7.5",
    "lodash.assign": "^3.2.0",
    "make-error": "^0.3.0",
    "symbol-enum": "^3.2.2"
  },
  "devDependencies": {
    "babel-cli": "^6.1.1",
    "babel-core": "^6.0.20",
    "babel-plugin-transform-es2015-destructuring": "^6.0.18",
    "babel-plugin-transform-flow-strip-types": "^6.0.14",
    "babel-plugin-transform-runtime": "^6.0.16",
    "babel-plugin-transform-strict-mode": "^6.0.15",
    "babel-plugin-undeclared-variables-check": "^6.0.14",
    "chai": "^1.9.2",
    "chokidar-cli": "^1.1.1",
    "glob": "^5.0.15",
    "ink-docstrap": "^0.5.2",
    "jsdoc": "^3.3.3",
    "jsdoc-oblivion": "0.0.4",
    "mkdirp": "^0.5.0",
    "mocha": "^2.0.1",
    "typedoc": "^0.3.12",
    "typescript": "^1.6.2"
  },
  "babel": {
    "plugins": [
      "transform-es2015-destructuring"
    ]
  }
}
